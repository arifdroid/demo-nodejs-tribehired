{
  "name": "tribehired-interview-node",
  "description": "node backend",
  "scripts": {
    "start": "nodemon --watch \"src/**/*.ts\" -e ts,json --exec \"ts-node --transpile-only ./src/server.ts\"",
    "test": "nodemon --exec \"cross-env NODE_ENV=test mocha -r ts-node/register ./src/**/*.test.ts --exit || exit 1\"",
    "build": "tsc",
    "db:create": "npm run build && node ./dist/database/migrations/create"
  },
  "dependencies": {
    "@types/node": "^14.14.22",
    "axios": "^0.21.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-rate-limit": "5.1.3",
    "helmet": "4.1.1",
    "lodash": "^4.17.20"
  },
  "private": true,
  "devDependencies": {
    "cross-env": "7.0.2",
    "mocha": "8.1.3",
    "nodemon": "2.0.4",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  }
}
